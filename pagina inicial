#include <stdio.h>

int main() {
    char tecla;
    printf("                                                                                    \n");
    printf("                                                                                    \n");
    printf("                                                                                    \n");

    printf("                                                                                          -:-%%: ....::::--*               \n");
    printf("                                                                                        ==     +@:           *              \n");
    printf("                                                                                       #-       +%%.           *             \n");
    printf("                                                                                      %%-         *%%.           #    %%       \n");
    printf("                                                                                     %%:          .@#.          .#%%*#        \n");
    printf("                                                                                    %%:          .#  #             =         \n");
    printf("                                                                                    *+:        .%%    *           --         \n");
